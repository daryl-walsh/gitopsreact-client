{
  "name": "gitopsreact-client",
  "version": "0.1.1",
  "description": "gitopsreact react front-end",
  "main": "index.js",
  "repository": "git@github.com:darylwalsh/gitopsreact-client.git",
  "author": "Daryl <daryl@blpc.us>",
  "license": "MIT",
  "scripts": {
    "api:init": "git submodule update --init",
    "api:install": "yarn install --cwd api",
    "api:serve": "yarn --cwd api run build && dotenv -e api/env/.env node api/dist/index.js",
    "api:update": "git submodule update --init --remote",
    "build": "rm -rf dist/ && webpack",
    "serve": "./scripts/serve.sh",
    "test:e2e": "yarn run test:e2e:all",
    "test:e2e:all": "yarn run test:e2e:chromium firefox",
    "test:e2e:chromium": "./scripts/e2e.test.sh chromium",
    "test:e2e:firefox": "./scripts/e2e.test.sh firefox"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chance": "^1.0.18",
    "chromedriver": "^75.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "cucumber": "^5.1.0",
    "dotenv-cli": "^2.0.0",
    "geckodriver": "^1.16.2",
    "node-fetch": "^2.6.0",
    "pushstate-server": "^3.1.0",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "string-replace-loader": "^2.2.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.4"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "deepmerge": "^3.3.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1"
  }
}
